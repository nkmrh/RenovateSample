{
    "extends": [
        "config:base"
    ],
    "rangeStrategy": "update-lockfile",
    "regexManagers": [{
        "fileMatch": ["^project.yml$"],
        "matchStrings": [
            "url: https:\\/\\/github\\.com\\/(?<depName>.*?)\\s*versions: (?<currentValue>.*)\\s"
        ],
        "datasourceTemplate": "github-releases"
    }]
}